<% client ||= @client %>
<% team ||= @team || client&.team %>
<%= render 'account/teams/breadcrumbs', team: team %>
<%= render 'account/shared/breadcrumb', label: t('.label'), url: [:account, team, :clients] %>
<% if client&.persisted? %>
  <%= render 'account/shared/breadcrumb', label: client.label_string, url: [:account, client] %>
<% end %>
<%= render 'account/shared/breadcrumbs/actions', only_for: 'clients' %>
